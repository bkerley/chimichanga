#include "init.h"

dispatch "+"
{
	long a,b,r;
	a = (long)stack_pop();
	b = (long)stack_pop();
	r = a + b;
	stack_push((stack_entry)r);
}

dispatch "*"
{
	long a,b,r;
	a = popl;
	b = popl;
	r = a * b;
	push(r);
}

dispatch "-"
{
	long a,b,r;
	a = popl;
	b = popl;
	r = b - a;
	push(r);
}

dispatch "/"
{
	long a,b,r;
	a = popl;
	b = popl;
	r = b / a;
	push(r);
}

dispatch "pop"
{
	long a;
	a = popl;
	printf("%ld\n",a);
}

dispatch "pop.x"
{
	long a;
	a = popl;
	printf("0x%lx\n",a);
}
